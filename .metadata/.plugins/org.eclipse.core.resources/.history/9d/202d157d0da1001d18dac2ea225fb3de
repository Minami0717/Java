
public class PrimeNumber {

	public static void main(String[] args) {
		int prime = 2;
		boolean isPrime = true;
		for (int i = 2; i < 100; i++) {
			for (int j = 2; j < i; j++) {
				if (i % j == 0) {
					isPrime = false;
				}
			}
			if (isPrime == true) {
				System.out.println(prime);
			}
		}
	}

}
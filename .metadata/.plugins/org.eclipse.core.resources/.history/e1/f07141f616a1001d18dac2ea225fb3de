import java.time.LocalTime;

public class PrimeNumber {

	public static void main(String[] args) {
		int prime = 2;
		boolean isPrime = true;
		System.out.println(LocalTime.now());
		for (int i = 2; i < 100; i++) {
			for (int j = 2; j < i; j++) {
				if (i % j == 0) {
					isPrime = false;
					break;
				}
				else {
					isPrime = true;
					prime = i;
				}
			}
			if (isPrime)
				System.out.println(prime);
		}
		System.out.println(LocalTime.now());
	}
}
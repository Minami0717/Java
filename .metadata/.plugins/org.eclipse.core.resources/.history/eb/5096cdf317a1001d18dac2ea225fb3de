import java.time.LocalTime;

public class PrimeNumber {

	public static void main(String[] args) {
		int prime = 2;
		
		System.out.println("시간 : " + LocalTime.now());
		for (int i = 2; i < 100; i++) {
			boolean isPrime = true;
			for (int j = 2; j < i; j++) {
				if (i % j == 0) {
					isPrime = false;
					break;
				}
			}
			if (isPrime)
				System.out.print(i+" ");
		}
		System.out.println("\n시간 : " + LocalTime.now());
	}
}